CopyFiles = ModemCopyFiles, SysCopyFiles, Dll32CopyFiles, UIUCopyFiles
 
[Install1.NT.Services]
AddService = usbser,  0x00000000, LowerFilter_Service_Inst
AddService = acfva,   0x00000000, svc_acf
AddService = mdmxsdk, 0x00000000, svc_sdk
AddService = dgcfltr, 0x00000000, DGCFilter_Service_Inst
