[LowerFilter_Service_Inst]
DisplayName   = "USB Modem Driver"
ServiceType   = 1
StartType     = 3
ErrorControl  = 0
ServiceBinary = %12%\usbser.sys

[svc_acf]
ServiceType   = 1
StartType     = 3
ErrorControl  = 0
ServiceBinary = %12%\ACFVA64.sys

[svc_sdk]
DisplayName   = "Modem SDK Driver"
ServiceType   = 1
StartType     = 2
ErrorControl  = 0
ServiceBinary = %12%\ACFSDK64.sys

[DGCFilter_Service_Inst]
DisplayName   = "DGC Filter Driver"
ServiceType   = 1
StartType     = 3
ErrorControl  = 0
ServiceBinary = %12%\ACFDCP64.sys
 
[XAudio_Driver_Service_Inst]
ServiceType    = 1
StartType      = 2 
ErrorControl   = 0 
ServiceBinary  = %12%\ACFXAU64.sys
 
[XAudio_UserMode_Service_Inst]
DisplayName    = %XAudio2.SVCDESC%
Description    = %XAudioDescription%
ServiceType    = 0x10
StartType      = 2
ErrorControl   = 1
ServiceBinary  = %11%\svchost.exe -k AcfXAudioService
AddReg         = XAudioService.AddReg
Security       = "D:(D;;GA;;;BG)(A;;GA;;;SY)(A;;GA;;;BA)(A;;GRGX;;;PU)(A;;GA;;;SO)(A;;GRRP;;;AU)"
StartName      = "NT AUTHORITY\LocalService"

[XAudioService.AddReg]
